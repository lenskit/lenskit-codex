{
  "imports": {
    "std/": "https://deno.land/std@0.224.0/",
    "aitertools": "https://deno.land/x/aitertools@0.5.0/mod.ts",
    "zod": "npm:zod@^3"
  },
  "fmt": {
    "lineWidth": 100,
    "indentWidth": 2
  },
  "lint": {
    "rules": {
      "tags": [
        "recommended"
      ],
      "exclude": [
        "prefer-const"
      ],
      "include": [
        "no-const-assign"
      ]
    }
  },
  "compilerOptions": {
    "strict": true,
    "lib": [
      "deno.window",
      "dom.asynciterable"
    ]
  }
}
